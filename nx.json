{
  "$schema": "packages/nx/schemas/nx-schema.json",
  "implicitDependencies": {
    "package.json": "*",
    ".eslintrc.json": "*",
    ".circleci/config.yml": "*",
    "tools/eslint-rules/**/*": "*"
  },
  "affected": {
    "defaultBase": "master"
  },
  "npmScope": "nrwl",
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/nx-cloud",
      "options": {
        "accessToken": "YzVhYjFiNzAtYTYxZS00OWM3LTlkOGYtZjRmOGRlNDY4MTJhfHJlYWQtd3JpdGU=",
        "cacheableOperations": [
          "build",
          "build-base",
          "test",
          "lint",
          "lint-base",
          "e2e",
          "sitemap"
        ],
        "useDaemonProcess": true,
        "runtimeCacheInputs": [
          "echo $SELECTED_CLI",
          "echo $NX_E2E_CI_CACHE_KEY"
        ],
        "cacheDirectory": "/tmp/nx-cache",
        "parallel": 1
      }
    }
  },
  "workspaceLayout": {
    "libsDir": "",
    "appsDir": ""
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "pluginsConfig": {
    "@nrwl/jest": {
      "hashingExcludesTestsOfDeps": true
    },
    "@nrwl/cypress": {
      "hashingExcludesTestsOfDeps": true
    }
  },
  "defaultProject": "dep-graph-client",
  "targetDefaults": {
    "build": {
      "dependsOn": ["build-base"]
    },
    "build-base": {
      "dependsOn": ["^build-base"]
    }
  }
}
