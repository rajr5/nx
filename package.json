{
  "name": "@nrwl/nx-source",
  "version": "7.8.1",
  "description": "Angular CLI power-ups for modern Web development",
  "homepage": "https://nx.dev",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "./scripts/package.sh --local",
    "commit": "git-cz",
